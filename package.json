{
  "name": "jqTerm",
  "productName": "jqTerm",
  "version": "1.9.0",
  "description": "A jq service",
  "main": "app",
  "scripts": {
    "app": "nodemon -x 'electron app'",
    "start": "node server.js",
    "dev": "nodemon server.js -i dist -i public -e js -V",
    "package": "electron-builder --dir",
    "build": "VERSION=$npm_package_version node ./build.js",
    "deploy": "now && now alias"
  },
  "dependencies": {
    "@remy/envy": "^4.0.2",
    "@remy/node-jq": "^1.1.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "doc-ready": "^1.0.4",
    "electron-about-window": "^1.14.0",
    "electron-context-menu": "^3.1.0",
    "electron-prompt": "^1.6.2",
    "electron-store": "^8.0.0",
    "electron-window-state": "^5.0.3",
    "express": "^4.17.1",
    "gists": "^2.0.0",
    "http-status-codes": "^2.1.4",
    "js-yaml": "^4.1.0",
    "json5": "^2.2.0",
    "lru-cache": "^6.0.0",
    "make-dir": "^3.1.0",
    "method-override": "^3.0.0",
    "node-machine-id": "^1.1.12",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "undefsafe": "^2.0.3",
    "uuid": "^8.3.2",
    "write-file-atomic": "^3.0.3"
  },
  "engines": {
    "node": "10"
  },
  "build": {
    "fileAssociations": [
      {
        "ext": "jq",
        "name": "jq script",
        "role": "Editor"
      },
      {
        "ext": "json",
        "name": "JSON",
        "role": "Viewer"
      }
    ],
    "appId": "com.remysharp.jqterm",
    "productName": "jqTerm",
    "mac": {
      "category": "public.app-category.development",
      "target": [
        "pkg",
        "dmg",
        "zip",
        "mas"
      ],
      "entitlements": "build/entitlements.mac.plist",
      "type": "distribution",
      "icon": "build/icon.icns"
    },
    "mas": {
      "entitlements": "build/entitlements.mas.plist",
      "type": "distribution",
      "category": "public.app-category.development"
    },
    "files": [
      "public/**/*",
      "lib/**/*",
      "node_modules/**/*",
      "app/**/*",
      "package.json",
      "embedded.provisionprofile"
    ]
  },
  "repository": {},
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "optionalDependencies": {},
  "author": "Remy Sharp",
  "devDependencies": {
    "electron": "^13.1.2",
    "electron-builder": "^22.11.7"
  }
}
